{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "ES2020",
        "checkJs": true,
        "allowSyntheticDefaultImports": true,
        "strictNullChecks": false,
        "strictFunctionTypes": false
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*",
        "*.cpp",
        "module_backups_*",
        "*_backup*.js"
    ],
    "include": [
        "*.js",
        "source*.cpp"
    ]
}