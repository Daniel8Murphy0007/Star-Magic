{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "bundler",
        "target": "ES2020",
        "checkJs": false,
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": false,
        "strictNullChecks": false,
        "strictFunctionTypes": false
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*",
        "*.cpp",
        "module_backups_*",
        "*_backup*.js"
    ],
    "include": [
        "index.js",
        "source*.js"
    ]
}